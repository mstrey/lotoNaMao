<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="br.nom.strey.maicon.lotonamao">

    <uses-sdk android:minSdkVersion="14" android:targetSdkVersion="16"/>

    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name="br.nom.strey.maicon.lotonamao.JogoListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/title_jogo_detail" android:name="br.nom.strey.maicon.lotonamao.JogoDetailActivity" android:parentActivityName=".JogoListActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".JogoListActivity"/>
        </activity>
    </application>

</manifest>
