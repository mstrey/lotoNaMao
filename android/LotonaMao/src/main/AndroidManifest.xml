<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.nom.strey.maicon.lotonamao"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="br.nom.strey.maicon.lotonamao.JogoListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="br.nom.strey.maicon.lotonamao.JogoDetailActivity"
            android:label="@string/title_jogo_detail"
            android:parentActivityName=".JogoListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".JogoListActivity" />
        </activity>
    </application>

</manifest>
